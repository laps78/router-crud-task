import axios from "axios";

async function getRequest(url, opts){
  let data = null;
  const responce = await axios.get(url + opts)
    .then(() => {
      console.log('getRequest responce: \n\n', responce);
      data = responce.data;
    })
    .catch((err) => {
      console.log('getRequest responce err: \n\n', err);
    }).finally(() => {
      return [...data];
    });
}

export default getRequest;